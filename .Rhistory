n = as.numeric(readline("Enter the number: "))
for (num in 2:n) {
is_Prime = TRUE
if (num > 2) {
for (i in 2:(num-1)) {
if (num %% i == 0) {
is_Prime = FALSE
break
}
}
}
if (is_Prime) cat(num, " ")
}
