n = readline("Enter the number : ")
cat("Prime Numbers : ")
for (num in 2:n){
is_Prime = TRUE
for(i in 2:sqrt(num)){
if(num %% i == 0){
is_Prime = FALSE
break
}
}
if(is_Prime == TRUE){
cat(num, " ")
}
}
n = readline("Enter the number : ")
cat("Prime Numbers : ")
for (num in 2:n){
is_Prime = TRUE
for(i in 2:floor(sqrt(num))){
if(num %% i == 0){
is_Prime = FALSE
break
}
}
if(is_Prime == TRUE){
cat(num, " ")
}
}
